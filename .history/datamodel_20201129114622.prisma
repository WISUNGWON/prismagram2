type User {
  id: ID! @id
  name: String!
  lastName: String!
  posts: [Post!]!
}

type Post {
  files: [Files!]!
  location: String
  caption: String!
  user: User!
  likes: [Like!]
  comments: [Comments!]!

}

type Like {
  user: User!
  post: Post!
}

type Comment {
  text: String!
  user: User!
  post: Post!
}

type File {
  url: String!
  post: Post!
}
