type User {
  id: ID! @id
  username: String! @unique
  email: String! @unique
  name: String!
  firstName: String @default(value: "")
  lastName: String!
  bio: String
  followers: [User!]!
  following:[User!]!
  posts: [Post!]!
  likes: [Like!]!
  Comments: [Commnets!]!
}

type Post {
  location: String
  files: [Files!]!
  caption: String!
  user: User!
  likes: [Like!]
  comments: [Comments!]!

}

type Like {
  user: User!
  post: Post!
}

type Comment {
  text: String!
  user: User!
  post: Post!
}

type File {
  url: String!
  post: Post!
}
